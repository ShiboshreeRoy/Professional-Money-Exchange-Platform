# Environment Configuration File
# Rename this file to .env and update with your actual values

# Database Configuration
DB_HOST=localhost
DB_USERNAME=root
DB_PASSWORD=
DB_NAME=website_db
DB_PORT=3306

# Application Settings
APP_NAME="Professional Website"
APP_ENV=local
APP_DEBUG=true
APP_URL=http://localhost/fff

# Security Settings
APP_KEY=your-secret-key-here-change-this
SESSION_LIFETIME=120

# Mail Configuration
MAIL_MAILER=smtp
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS=admin@example.com
MAIL_FROM_NAME="${APP_NAME}"

# Cache Settings
CACHE_DRIVER=file
SESSION_DRIVER=file
QUEUE_CONNECTION=sync

# Logging
LOG_CHANNEL=stack
LOG_LEVEL=debug

# API Keys (if needed)
GOOGLE_ANALYTICS_ID=
FACEBOOK_APP_ID=
TWITTER_API_KEY=

# Payment Gateway (if needed)
STRIPE_KEY=
STRIPE_SECRET=
PAYPAL_CLIENT_ID=
PAYPAL_SECRET=